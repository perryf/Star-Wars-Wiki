<div class="edit-form">
  <h2>Create a new Alliance</h2>
</div>
  <%= form_for (@alliance) do |f| %>
  <div class="edit-form">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>

  <div class="edit-form">
    <%= f.label :img_url %>
    <%= f.text_field :img_url %>
  </div>

  <div class="edit-form">
    <%= f.submit %>
  </div>
<% end %>
